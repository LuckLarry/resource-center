      <div class="col-md-4 form-group field">
        <%= f.label :sku %><br>
        <%= f.text_field :sku,class: 'form-control' %>
      </div>
    </div>
    <div class="row" style="margin-top:20px;margin-left:20px;">
      <div class="actions">
        <%= f.submit "确定",class: 'btn btn-default' %>
